{
    "name": "Mopidy and Snapcast",
    "version": "0.2.4",
    "slug": "mopidy-cast",
    "description": "mopidy music server with snapcast server",
    "url": "https://github.com/signaleleven/hassio-addons/tree/master/mopidy_cast",
    "startup": "application",
    "boot": "auto",
    "options": {"local_scan": true,
	        "options": []},
    "schema": {
	"local_scan": "bool?",
	"options": [{"name": "str", "value": "str"}]
    },
    "devices": [],
    "ports": {"6680/tcp": 6681, "6600/tcp": 6601, "1704/tcp": 1704, "1705/tcp": 1705},
    "map": ["share"],
    "arch": ["amd64", "armhf"],
    "tmpfs": "size=10m,rw",
    "webui": "http://[HOST]:[PORT:6681]",
    "host_ipc": true
}
